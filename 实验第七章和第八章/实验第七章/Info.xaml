        <Label x:Name="label" Content="姓名：" HorizontalAlignment="Left" Margin="168,87,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="基本信息：" HorizontalAlignment="Left" Margin="168,141,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="其他信息：" HorizontalAlignment="Left" Margin="168,205,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label3" Content="学生基本信息：" HorizontalAlignment="Left" Margin="86,34,0,0" VerticalAlignment="Top"/>

    </Grid>
